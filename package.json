{
  "name": "@stedi/integrations-sdk",
  "version": "0.1.24",
  "description": "Stedi Integrations SDK",
  "type": "module",
  "scripts": {
    "build": "tsc && chmod +x ./dist/cli.js",
    "test": "ava",
    "coverage": "c8 node ./node_modules/.bin/ava",
    "lint": "npx eslint --fix \"src/**/*.ts\""
  },
  "files": [
    "dist/**/*",
    "tsconfig.json",
    ".env.example",
    ".gitignore",
    "project-readme.md"
  ],
  "types": "./dist/index.d.ts",
  "source": "src/index.ts",
  "main": "./dist/index.js",
  "bin": {
    "stedi-integrations": "./dist/cli.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./testing": "./dist/testing/index.js",
    "./clients": "./dist/clients/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index"
      ],
      "testing": [
        "./dist/testing/index"
      ]
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@ava/typescript": "4.1.0",
    "@smithy/types": "1.2.0",
    "@smithy/util-stream-node": "2.0.4",
    "@stedi/sdk-client-as2": "0.4.26",
    "@stedi/sdk-client-buckets": "0.4.26",
    "@stedi/sdk-client-edi-translate": "0.4.26",
    "@stedi/sdk-client-events": "0.4.26",
    "@stedi/sdk-client-functions": "0.4.26",
    "@stedi/sdk-client-guides": "0.4.26",
    "@stedi/sdk-client-identity": "0.4.26",
    "@stedi/sdk-client-mappings": "0.4.26",
    "@stedi/sdk-client-partners": "0.4.26",
    "@stedi/sdk-client-sftp": "0.4.26",
    "@stedi/sdk-client-stash": "0.4.26",
    "@stedi/sdk-token-provider-aws-identity": "0.2.8",
    "@types/cli-progress": "3.11.0",
    "@types/fs-extra": "11.0.1",
    "@types/node": "20.4.6",
    "@types/yargs": "17.0.24",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "ava": "5.3.1",
    "aws-sdk-client-mock": "3.0.0",
    "c8": "8.0.1",
    "chalk": "5.3.0",
    "cli-select": "1.1.2",
    "dotenv": "16.3.1",
    "esbuild": "0.18.17",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "4.2.1",
    "fs-extra": "11.1.1",
    "jszip": "3.10.1",
    "prettier": "2.8.8",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "undici": "5.22.1",
    "yargs": "17.7.2",
    "zod": "3.21.4"
  },
  "prettier": {
    "useTabs": false
  },
  "ava": {
    "environmentVariables": {},
    "typescript": {
      "rewritePaths": {
        "src/": "dist/"
      },
      "compile": "tsc"
    }
  },
  "volta": {
    "node": "18.17.0",
    "npm": "9.8.1"
  }
}
