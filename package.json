{
  "name": "@stedi/integrations-sdk",
  "version": "0.1.10",
  "description": "Stedi Integrations SDK",
  "type": "module",
  "scripts": {
    "build": "tsc && chmod +x ./dist/cli.js",
    "test": "ava",
    "coverage": "c8 node ./node_modules/.bin/ava",
    "lint": "npx eslint --fix \"src/**/*.ts\""
  },
  "files": [
    "dist/**/*",
    "tsconfig.json",
    ".env.example",
    ".gitignore",
    "project-readme.md"
  ],
  "types": "./dist/index.d.ts",
  "source": "src/index.ts",
  "main": "./dist/index.js",
  "bin": {
    "stedi-integrations": "./dist/cli.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./testing": "./dist/testing/index.js",
    "./clients": "./dist/clients/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index"
      ],
      "testing": [
        "./dist/testing/index"
      ]
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@ava/typescript": "4.0.0",
    "@aws-sdk/client-s3": "3.329.0",
    "@aws-sdk/util-stream-node": "3.329.0",
    "@stedi/sdk-client-as2": "0.2.33",
    "@stedi/sdk-client-buckets": "0.2.33",
    "@stedi/sdk-client-edi-translate": "0.2.33",
    "@stedi/sdk-client-events": "0.2.33",
    "@stedi/sdk-client-functions": "0.2.32",
    "@stedi/sdk-client-guides": "0.2.32",
    "@stedi/sdk-client-identity": "0.2.32",
    "@stedi/sdk-client-mappings": "0.2.32",
    "@stedi/sdk-client-partners": "0.2.32",
    "@stedi/sdk-client-sftp": "0.2.32",
    "@stedi/sdk-client-stash": "0.2.32",
    "@types/cli-progress": "3.11.0",
    "@types/fs-extra": "11.0.1",
    "@types/node": "18.15.11",
    "@types/yargs": "17.0.24",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "ava": "5.2.0",
    "aws-sdk-client-mock": "2.1.1",
    "c8": "7.13.0",
    "chalk": "5.2.0",
    "cli-select": "1.1.2",
    "dotenv": "16.0.3",
    "esbuild": "0.17.19",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "fs-extra": "11.1.1",
    "jszip": "3.10.1",
    "prettier": "2.8.8",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "undici": "5.22.1",
    "yargs": "17.7.2",
    "zod": "3.21.4"
  },
  "prettier": {
    "useTabs": false
  },
  "ava": {
    "environmentVariables": {},
    "typescript": {
      "rewritePaths": {
        "src/": "dist/"
      },
      "compile": "tsc"
    }
  },
  "volta": {
    "node": "18.16.0",
    "npm": "9.6.7"
  }
}
